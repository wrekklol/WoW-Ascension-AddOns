			<Anchor point="BOTTOMRIGHT">
				<Offset x="-100" y="100" />
			</Anchor>
		</Anchors>
		<Backdrop bgFile="Interface\AddOns\PortalHelpers\Textures\Square_White.tga">
			<Color />
			<BorderColor a="0" />
		</Backdrop>
		<Layers>
			<Layer level="OVERLAY">
				<FontString name="Party1HealthText" inherits="GameFontNormal" text="100">
					<Size x="30" y="20" />
					<Anchors>
						<Anchor x="-105" y="78" point="BOTTOMRIGHT" relativeTo="PartyFrame" />
					</Anchors>
				</FontString>
				<FontString name="Party2HealthText" inherits="GameFontNormal" text="100">
					<Size x="30" y="20" />
					<Anchors>
						<Anchor y="-20" point="BOTTOMRIGHT" relativeTo="Party1HealthText" />
