						<Anchor y="-20" point="BOTTOMRIGHT" relativeTo="Party4HealthText" />
					</Anchors>
				</FontString>
				<FontString name="TankIndicator" inherits="GameFontNormal" hidden="true" text="X">
					<Size x="20" y="20" />
					<Anchors>
						<Anchor x="-141" y="78" point="BOTTOMRIGHT" relativeTo="PartyFrame" />
					</Anchors>
				</FontString>
			</Layer>
		</Layers>
		<Frames>
			<StatusBar name="Party1Health" toplevel="true" minValue="0" maxValue="100" defaultValue="50">
				<Size x="100" y="15" />
				<Anchors>
					<Anchor x="-1" y="81" point="BOTTOMRIGHT" relativeTo="PartyFrame" />
				</Anchors>
				<Backdrop bgFile="Interface\AddOns\PortalHelpers\Textures\Square_White.tga">
					<Color />
				</Backdrop>
				<BarTexture file="Interface\AddOns\PortalHelpers\Textures\Square_White.tga" />
				<BarColor g="1" />
			</StatusBar>
			<StatusBar name="Party2Health" toplevel="true" minValue="0" maxValue="100" defaultValue="50">
				<Size x="100" y="15" />
